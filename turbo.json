{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "env": ["GEMINI_API_KEY", "NODE_ENV"]
    },
    "check-types": {},
    "dev": {
      "persistent": true,
      "cache": false,
      "env": ["GEMINI_API_KEY", "NODE_ENV"]
    },
    "lint": {
      "outputs": [],
      "env": ["NODE_ENV"]
    },
    "check": {
      "dependsOn": ["lint", "check-types"],
      "outputs": []
    }
  }
}
